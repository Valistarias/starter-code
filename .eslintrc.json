{
    "extends": "google",
    "parser": "babel-eslint",
    "rules": {
        "require-jsdoc": 0,
        "valid-jsdoc": 0,
        "linebreak-style": ["error", "windows"],
        "padded-blocks": ["error", { 
            "blocks": "never",
            "classes": "always" }],
        "no-tabs" : 0,
        "space-in-parens": ["error", "always"]
    }
}